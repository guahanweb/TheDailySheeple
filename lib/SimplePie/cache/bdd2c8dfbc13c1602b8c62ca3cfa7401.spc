:8:"xml_lang";s:0:"";s:5:"child";a:4:{s:27:"http://www.w3.org/2005/Atom";a:8:{s:2:"id";a:1:{i:0;a:5:{s:4:"data";s:70:"tag:blogger.com,1999:blog-3699593565876363580.post-8170064861702693939";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:9:"published";a:1:{i:0;a:5:{s:4:"data";s:29:"2012-09-10T07:31:00.001-07:00";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:7:"updated";a:1:{i:0;a:5:{s:4:"data";s:29:"2012-09-17T04:54:13.794-07:00";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:8:"category";a:7:{i:0;a:5:{s:4:"data";s:0:"";s:7:"attribs";a:1:{s:0:"";a:2:{s:6:"scheme";s:31:"http://www.blogger.com/atom/ns#";s:4:"term";s:9:"pathogens";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}i:1;a:5:{s:4:"data";s:0:"";s:7:"attribs";a:1:{s:0:"";a:2:{s:6:"scheme";s:31:"http://www.blogger.com/atom/ns#";s:4:"term";s:11:"novel virus";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}i:2;a:5:{s:4:"data";s:0:"";s:7:"attribs";a:1:{s:0:"";a:2:{s:6:"scheme";s:31:"http://www.blogger.com/atom/ns#";s:4:"term";s:11:"new disease";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}i:3;a:5:{s:4:"data";s:0:"";s:7:"attribs";a:1:{s:0:"";a:2:{s:6:"scheme";s:31:"http://www.blogger.com/atom/ns#";s:4:"term";s:5:"ticks";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}i:4;a:5:{s:4:"data";s:0:"";s:7:"attribs";a:1:{s:0:"";a:2:{s:6:"scheme";s:31:"http://www.blogger.com/atom/ns#";s:4:"term";s:8:"Missouri";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}i:5;a:5:{s:4:"data";s:0:"";s:7:"attribs";a:1:{s:0:"";a:2:{s:6:"scheme";s:31:"http://www.blogger.com/atom/ns#";s:4:"term";s:18:"Heartlands disease";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}i:6;a:5:{s:4:"data";s:0:"";s:7:"attribs";a:1:{s:0:"";a:2:{s:6:"scheme";s:31:"http://www.blogger.com/atom/ns#";s:4:"term";s:3:"CDC";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:5:"title";a:1:{i:0;a:5:{s:4:"data";s:53:"Lone Star Tick is Implicated in Yet ANOTHER New Virus";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:7:"content";a:1:{i:0;a:5:{s:4:"data";s:14510:"<span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;"><br /></span>
<br />
<table cellpadding="0" cellspacing="0" class="tr-caption-container" style="float: right; text-align: right;"><tbody>
<tr><td style="text-align: center;"><img alt="Female lone star tick" border="1" height="175" src="http://www.ent.iastate.edu/images/ticks/aamer/aamerfdsm.jpg" style="margin-left: auto; margin-right: auto;" width="200" /></td></tr>
<tr><td class="tr-caption" style="text-align: center;">lone star tick</td></tr>
</tbody></table>
<span class="Apple-style-span" style="font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;">Tests have concluded that an entirely new virus has been found in Missouri. Two farmers, the only confirmed cases so far both recovered but were seriously ill and required hospitalisation. One man, Robert Wonderly still suffers from the after effects of the disease two years after contracting it.</span><span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;"></span><br />
<span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;"></span><br />
<div>
<span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;"><br /></span></div>
<span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;">
</span>
<br />
<div>
<span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;">The cases, which are independent of each other were though to be ehrliciosis, a bacterial disease but when antibiotics failed to cause any form of improvement in the men's conditions microscopy and cultures confirmed it wasn't ehrliciosis or any other disease previously seen.</span></div>
<span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;">
</span>
<br />
<div>
<span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;"><br /></span></div>
<span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;">
</span>
<br />
<div>
<span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;">The closest match to a known disease was to that of SFTS ( severe fever with thrombocytopenia syndrome) which was discovered in China last year and has a 12% mortality rate. Thrombocytopenia causes a profound drop in platelets which are used for clotting. This results in bleeding , either internally or into the skin which presents as profuse bruising. Both men had very low platelets, but neither suffered bleeding and this in itself is considered &nbsp;odd by the doctors.</span></div>
<span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;">
</span>
<br />
<div>
<span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;"><br /></span></div>
<span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;">
</span>
<br />
<div>
<span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;">The CDC have named it Heartland virus and their department for viral pathogens have confirmed its novel status. Whilst acknowledging that it is similar to other phleboviuses there are enough differences in the proteins present to classify it as a new species.</span></div>
<span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;">
</span>
<br />
<div>
<span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;"><br /></span></div>
<span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;">
</span>
<div>
<span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;">Although almost sure that it is indeed a tick borne virus researchers have spent almost two years catching ticks and taking blood samples from the population and the animals in the area it was found. It is most likely the &nbsp;Amblyomma americanum or the lone star tick is the one most likely involved in passing the virus to humans as this tick is the most prevalent in the area. This tick is reddish brown in colour, is the size of a sesame seed and had a cream patch, resembling a star in the centre of its back. This tick like many others will happily feed from humans as well as dogs, deer and many other wild and domestic animals.</span></div>
<span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;">
<div>
<br /></div>
<div>
To date no ticks carrying the virus have been isolated. It is also not known if the disease can be passed from person to person, though that was not the case with the first two victims, or if it could prove fatal in some people.&nbsp;</div>
<div>
<br /></div>
<div>
<span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.0898438); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.292969);"><br /></span>
<br />
<div class="separator" style="clear: both; text-align: center;">
<a href="http://us.cdn2.123rf.com/168nwm/kukhunthod/kukhunthod1207/kukhunthod120700072/14474936-blood-tube-use-for-testing-in-laboratory-for-diagnosis-disease.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img alt="Blood_bottle : blood tube use for testing in laboratory for diagnosis disease" border="0" height="257" id="14474936" src="http://us.cdn2.123rf.com/168nwm/kukhunthod/kukhunthod1207/kukhunthod120700072/14474936-blood-tube-use-for-testing-in-laboratory-for-diagnosis-disease.jpg" width="320" /></a></div>
</div>
</span><span class="Apple-style-span" style="font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;"><div>
<span class="Apple-style-span" style="font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;"><br /></span></div>
</span><span class="Apple-style-span" style="font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;">Researchers now plan to use blood samples stored from patients they were unable to make a definitive diagnosis on in an attempt to see if any of those patients had Heartlands. This will also give them an idea of the numbers suffering from it and the geographic spread of the disease. Doctors are concerned that as the symptoms of Heartlands are so general and could be attributed to many causes, the prevalence in the population may well be much higher than previously thought.</span><span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;">
<div>
<br /></div>
<div>
Symptoms</div>
<div>
<br /></div>
<div>
* general fatigue worsening with time</div>
<div>
* fever</div>
<div>
* loss of appetite</div>
<div>
* diarrhoea&nbsp;</div>
<div>
* muscle aches</div>
<div>
* thrombocytopenia ( may present as bruising)</div>
<div>
<br /></div>
<div>
Details of the treatment regimen for the disease have not been released at this time. If treatment of other viral conditions with those symptoms are anything to go by it will most likely consist of rehydration therapy, fever reduction, pain relief, blood transfusion if required and ventilatory support if required.</div>
<div>
<br />
Doctors advise avoiding tick bites in the first place by using repellents containing 20% DEET.<br />
<br /></div>
<div>
Details of the virus and research into it &nbsp;can be found in the August 2012 edition of The New England Journal of Medicine.<br />
<br /></div>
</span><span class="Apple-style-span" style="font-family: Noteworthy; font-size: medium;"><span class="Apple-style-span" style="line-height: 24px;"><b><br /></b></span></span><span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;"><div>
<a href="http://t3.gstatic.com/images?q=tbn:ANd9GcTQiGmBgxK1nlaEgyRGDwbgtM7ImP46N1l7cbFaxQULbnVl1jo9" imageanchor="1" style="clear: left; float: left; margin-bottom: 1em; margin-right: 1em;"><img alt="" border="0" class="rg_hi uh_hi" data-height="189" data-width="267" height="189" id="rg_hi" src="http://t3.gstatic.com/images?q=tbn:ANd9GcTQiGmBgxK1nlaEgyRGDwbgtM7ImP46N1l7cbFaxQULbnVl1jo9" style="height: 189px; width: 267px;" width="267" /></a>The lone star tick has also recently been sited as the cause of a strange allergy in Virginia. People bitten by it have found themselves unable to eat red meat. Researchers say they have logged over 1000 cases of people who have been bitten by the tick no longer being able to eat red meat. Those affected break out in hives that burn and itch intensively 3-4 hours after consuming beef, pork or lamb. Chicken and turkey do not affect them at all. Scientists have no idea why this is occurring and research is ongoing at this time.</div>
</span><span class="Apple-style-span" style="-webkit-composition-fill-color: rgba(130, 98, 83, 0.09375); -webkit-composition-frame-color: rgba(191, 107, 82, 0.496094); -webkit-tap-highlight-color: rgba(26, 26, 26, 0.296875); -webkit-text-size-adjust: auto; font-family: Noteworthy; font-size: 18px; font-weight: bold; line-height: 24px;">
<div>
<br /></div>
<div>
